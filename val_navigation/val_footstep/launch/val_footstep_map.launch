<launch>
  
  
  <node name="rviz" pkg="rviz" type="rviz" args="-d $(find val_footstep)/config/rviz_footstep_planning.rviz"/>
  <node name="footstep_planner" pkg="val_footstep" type="footstep_node" respawn="true" >
    <rosparam file="$(find val_footstep)/config/planning_params.yaml" command="load" />
    <rosparam file="$(find val_footstep)/config/planning_params_asimo.yaml" command="load" />
    <rosparam file="$(find val_footstep)/config/footsteps_asimo.yaml" command="load" />
  </node>

</launch>
