<?xml version="1.0"?>

<launch>
  <arg name="namespace" default="val_footstep_planner" />
  <group ns="$(arg namespace)">
    <!-- start up footstep planning system -->
    <include file="$(find vigir_footstep_planning)/launch/footstep_planning_basics.launch" />

    <!-- start footstep planner -->
    <node name="val_footstep_planner" pkg="vigir_global_footstep_planner" type="global_footstep_planner_node" respawn="true" output="screen" />

    <!-- load plugin descriptions and sets from YAML file to parameter server -->
    <rosparam file="$(find val_3dfootstep)/config/plugin_descriptions.yaml" command="load" />
    <rosparam file="$(find val_3dfootstep)/config/plugin_sets.yaml" command="load" />
    
    <!-- specify where to load robot specific parameters -->
    <rosparam file="$(find val_3dfootstep)/config/robot_params.yaml" command="load" />
    <param name="params_path" value="$(find val_3dfootstep)/config/planner/" />
    <param name="default_params" value="default" />

  </group>
</launch>
